-load humanize i18n

-if not paginator or paginator.num_pages <= 1
  -blocktrans trimmed with count=object_list|length|intcomma count cc=object_list|length
    {{ count }} result
    -plural
      {{ count }} results
-else
  -blocktrans trimmed with start=page_obj.start_index|intcomma end=page_obj.end_index|intcomma total=paginator.count|intcomma
    Results {{ start }} - {{ end }} of {{ total }}
