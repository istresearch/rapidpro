{% extends "smartmin/base.html" %}

-load compress temba i18n

-block page-title
  {{object.name}}

-block content
    #flow-editor

-block extra-style
  -compress css
    {% lessblock %}
      :plain
        @import "flow_editor_next.less";
    {% endlessblock %}

-block extra-script
  {{ block.super }}

  -compress js
    %script{type:'text/javascript', src:"{{ STATIC_URL }}js/flow-editor/dist/flow-editor.js"}

